{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "rules": {
    "default-case": 2,
    "eqeqeq": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-undef": 1,
    "no-unused-vars": 1,
    "quotes": [2, "single", "avoid-escape"],
    "strict": [2, "global"],
    "yoda": [2, "never"]
  }
}
